<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <application>
        <resource-bundle>
            <base-name>/Bundle</base-name>
            <var>bundle</var>
        </resource-bundle>
    </application>
    <navigation-rule>
        <from-view-id>/paginas/Login.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>home</from-outcome>
            <to-view-id>/paginas/home.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/paginas/loginadmin.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>home2</from-outcome>
            <to-view-id>/paginas/usuario/home2.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/paginas/admin/Create.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>homeR</from-outcome>
            <to-view-id>/paginas/home.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/paginas/home.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>homeuser</from-outcome>
            <to-view-id>/paginas/usuario/HomeUser.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
